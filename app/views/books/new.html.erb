<h1>Books</h1>
<% @books.each do |book| %>
<%= book.title %>
<%= book.body %>

<% end %>




<h1>New book</h1>
<%= form_with model: @book, url: '/books', method: :post do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <br><%= f.submit 'Create Book' %>
<% end %>